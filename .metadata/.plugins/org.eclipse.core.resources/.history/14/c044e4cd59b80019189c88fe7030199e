package SBA5_6;

public class Square extends Rectangle{

	float side;

	public Square(float side) {
		super(getLength(), getWidth());
		// TODO Auto-generated constructor stub
		
		this.side = getLength();
		this.side = getWidth();
		
		
	}

	public float getSide() {
		return side;
	}

	public void setSide(float side) {
		this.side = side;
	}
	
	

}

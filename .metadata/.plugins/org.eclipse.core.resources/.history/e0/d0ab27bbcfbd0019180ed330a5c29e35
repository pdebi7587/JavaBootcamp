package reviewWeek6;

public class ChessBoard {
	
	public static void main(String [] args) {
		
		for (int i=0; i<8;i++) {
			for (int j=0; j<4;j++) {
				if (j%2 == 0) {
				System.out.print("B W ");
				}
				else {
					System.out.print("W B ");
				}
			}
			
			System.out.println();
		}
		
		
		
		
		
	}

}

package reviewWeek6;

import java.util.Arrays;

public class RandomShuffle {
	
	public static void main (String [] args) {
		
		char [] arr = {'a', 'p', 's', 'r', 'z'};
		
		System.out.println(Arrays.toString(arr));
		
		for (int i=0; i<10;i++) {
		
		Arrays.sort(arr, 0, 4);
		
		System.out.println(Arrays.toString(arr));
		
		}
		
		
		
		
	}

}
